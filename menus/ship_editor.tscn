script/source = "extends Node3D



var selected_item : Node3D
var dragging := false



@onready var camera := $camera/Camera3D



func _ready():
	camera.make_current()
	load_ship()



func _unhandled_input(event):
	if event is InputEventMouseMotion and Input.is_action_pressed(\"Shoot Secondary\"):
		$camera.global_rotate(Vector3.UP, event.relative.x * -0.002)
		$camera.global_rotate($camera.global_basis.z.normalized(), event.relative.y * -0.002)
	
	if event is InputEventKey and event.keycode == KEY_BACKSPACE:
		if selected_item != null and !selected_item is Camera3D:
			selected_item.free()
			$\"part list\".recalculate()
			deselect()
	
	if event is InputEventMouseButton and event.button_index == MOUSE_BUTTON_WHEEL_UP:
		camera.position.x = move_toward(camera.position.x, -2.0, 0.5)
	if event is InputEventMouseButton and event.button_index == MOUSE_BUTTON_WHEEL_DOWN:
		camera.position.x = move_toward(camera.position.x, -20.0, 0.5)
	
	
	if event is InputEventMouseButton and event.button_index == MOUSE_BUTTON_LEFT and event.pressed:
		if dragging:
			selected_item.global_position = selected_item.global_position.snapped(Vector3(1, 1, 1))
			dragging = false
			selected_item.collision_layer = 0b0001
		else:
			var from = camera.project_ray_origin(get_viewport().get_mouse_position())
			var to = from + camera.project_ray_normal(get_viewport().get_mouse_position()) * 100
			var ray_query = PhysicsRayQueryParameters3D.create(from, to, 0b1)
			var raycast = get_world_3d().direct_space_state.intersect_ray(ray_query)
				
			if raycast.is_empty():
				deselect()
			else:
				if raycast.collider == $\"ship camera\":
					select_item(Ship.camera)
				else:
					select_item(raycast.collider)



func _process(delta):
	if selected_item == null:
		$\"rotate prompt\".hide()
		return
	$\"rotate prompt\".show()
	
	if dragging:
		var from = camera.project_ray_origin(get_viewport().get_mouse_position())
		var to = from + camera.project_ray_normal(get_viewport().get_mouse_position()) * 100
		var ray_query = PhysicsRayQueryParameters3D.create(from, to, 0b1)
		var raycast = get_world_3d().direct_space_state.intersect_ray(ray_query)
		
		if raycast.is_empty() or raycast.collider == $\"ship camera\":
			selected_item.global_position = camera.global_position + camera.project_ray_normal(get_viewport().get_mouse_position()) * camera.global_position.distance_to(Ship.global_position)
		else:
			selected_item.global_position = (raycast.position + raycast.normal * 0.99).snapped(Vector3(1, 1, 1))
	
	if Input.is_action_just_pressed(\"Forward\"):
		selected_item.global_rotate(global_basis.x, -PI / 2)
	if Input.is_action_just_pressed(\"Back\"):
		selected_item.global_rotate(global_basis.x, PI / 2)
	if Input.is_action_just_pressed(\"Left\"):
		selected_item.global_rotate(global_basis.y, PI / 2)
	if Input.is_action_just_pressed(\"Right\"):
		selected_item.global_rotate(global_basis.y, -PI / 2)
	if Input.is_action_just_pressed(\"Roll Left\"):
		selected_item.global_rotate(global_basis.z, PI / 2)
	if Input.is_action_just_pressed(\"Roll Right\"):
		selected_item.global_rotate(global_basis.z, -PI / 2)



func load_ship():
	$\"ship camera\".global_position = Ship.camera.global_position



func new_ship():
	$unsaved.hide()
	Ship.load_data({\"Colors\": [{\"R\": 1, \"G\": 1, \"B\": 1, \"Roughness\": 1, \"Metallic\": 0}], \"Parts\": []}, \"Untitled Ship\")
	deselect()



func select_item(item : Node3D):
	selected_item = item
	if !item is Camera3D:
		$\"part editor\".load_materials(item)



func deselect():
	selected_item = null
	dragging = false
	$\"part editor\".hide()



func add_part(path : String):
	var part = load(path).instantiate()
	Ship.add_child(part)
	
	select_item(part)
	move_part()


func move_part():
	dragging = true
	selected_item.collision_layer = 0b0000



func _on_exit_button_down():
	GameManager.start_planet()

"

