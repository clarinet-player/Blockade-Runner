script/source = "extends VBoxContainer



var available_parts := {}



func _ready():
	for part in GameManager.parts.keys():
		var new = $HBoxContainer.duplicate()
		add_child(new)
		new.show()
		new.name = part
		new.get_node(\"Label\").text = part
		new.gui_input.connect(add_part.bind(part))
		available_parts[part] = GameManager.parts[part]
	
	$HBoxContainer.hide()
	recalculate()



func add_part(event, part : String):
	if event is InputEventMouseButton and event.button_index == MOUSE_BUTTON_LEFT and event.pressed and available_parts[part] > 0:
		get_parent().call_deferred(\"add_part\", GameManager.part_data[part].file)
		call_deferred(\"recalculate\")



func recalculate():
	for part in GameManager.parts.keys():
		available_parts[part] = GameManager.parts[part]
	
	for child in Ship.get_children():
		if \"type\" in child:
			if available_parts.has(child.type):
				available_parts[child.type] -= 1
			else:
				available_parts[child.type] = -1
	
	for child in get_children():
		if child.name in available_parts:
			child.get_node(\"Label2\").text = \"x\" + str(available_parts[child.name])
"

