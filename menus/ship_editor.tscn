script/source = "extends VBoxContainer



var mat : StandardMaterial3D

var r := 1.0
var g := 1.0
var b := 1.0
var roughness := 1.0
var metallic := 0.0



func _ready():
	$Button.button_down.connect(hide)



func load_material(index : int):
	show()
	mat = Ship.color_scheme[index]
	$SubViewportContainer/SubViewport/MeshInstance3D.mesh.material = mat
	
	r = mat.albedo_color.r
	g = mat.albedo_color.g
	b = mat.albedo_color.b
	roughness = mat.roughness
	metallic = mat.metallic
	
	$red/HSlider.value = r
	$green/HSlider.value = g
	$blue/HSlider.value = b
	$roughness/HSlider.value = roughness
	$metallic/HSlider.value = metallic



func new_material():
	show()
	mat = StandardMaterial3D.new()
	Ship.color_scheme.append(mat)
	$SubViewportContainer/SubViewport/MeshInstance3D.mesh.material = mat
	
	r = 1
	g = 1
	b = 1
	roughness = 1
	metallic = 0
	
	$red/HSlider.value = r
	$green/HSlider.value = g
	$blue/HSlider.value = b
	$roughness/HSlider.value = roughness
	$metallic/HSlider.value = metallic



func _process(delta):
	if !visible:
		return
	
	r = $red/HSlider.value
	g = $green/HSlider.value
	b = $blue/HSlider.value
	roughness = $roughness/HSlider.value
	metallic = $metallic/HSlider.value
	
	mat.albedo_color.r = r
	mat.albedo_color.g = g
	mat.albedo_color.b = b
	mat.roughness = roughness
	mat.metallic = metallic
"

