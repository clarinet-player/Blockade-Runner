script/source = "extends VBoxContainer



var selected_part : Node3D
var current_index : int



func load_materials(part : Node3D):
	show()
	$Label2.hide()
	$HBoxContainer2.hide()
	current_index = -1
	selected_part = part
	
	for child in $HBoxContainer.get_children():
		if child != $HBoxContainer/ColorRect:
			child.queue_free()
	
	var index = 0
	for color in part.color_indexes:
		var mat = Ship.color_scheme[color]
		if index == 0:
			$HBoxContainer/ColorRect.color = Color(mat.albedo_color.r, mat.albedo_color.g, mat.albedo_color.b)
			$HBoxContainer/ColorRect.gui_input.connect(input_part.bind(0))
		else:
			var rect = $HBoxContainer/ColorRect.duplicate(0)
			rect.color = Color(mat.albedo_color.r, mat.albedo_color.g, mat.albedo_color.b)
			$HBoxContainer.add_child(rect)
			rect.gui_input.connect(input_part.bind(part.color_indexes.find(color)))
		index += 1
	
	$Name.text = part.type
	$Stats.text = \"Mass: \" + str(part.mass) + \" HP: \" + str(part.health)
	if \"Thrust\" in part.attributes:
		$Attributes.show()
		$Attributes.text = \"Thrust: \" + str(part.attributes.Thrust.length())
	elif !part.attributes.is_empty():
		$Attributes.show()
		$Attributes.text = part.attributes.keys()[0] + \": \" + str(part.attributes[part.attributes.keys()[0]])
	else:
		$Attributes.hide()



func input_part(event, index):
	if event is InputEventMouseButton and event.button_index == MOUSE_BUTTON_LEFT and event.pressed:
		current_index = index
		
		for child in $HBoxContainer2.get_children():
			if child != $HBoxContainer2/ColorRect and child != $HBoxContainer2/Button:
				child.queue_free()
		
		for mat in Ship.color_scheme:
			if mat == Ship.color_scheme[0]:
				$HBoxContainer2/ColorRect.color = Color(mat.albedo_color.r, mat.albedo_color.g, mat.albedo_color.b)
				$HBoxContainer2/ColorRect.gui_input.connect(input_picker.bind(0))
			else:
				var rect = $HBoxContainer2/ColorRect.duplicate(0)
				$HBoxContainer2.add_child(rect)
				rect.color = Color(mat.albedo_color.r, mat.albedo_color.g, mat.albedo_color.b)
				rect.gui_input.connect(input_picker.bind(Ship.color_scheme.find(mat)))
		
		$HBoxContainer2.move_child($HBoxContainer2/Button, -1)
		$Label2.show()
		$HBoxContainer2.show()



func input_picker(event, index):
	if event is InputEventMouseButton and event.button_index == MOUSE_BUTTON_LEFT and event.pressed:
		
		selected_part.color_indexes[current_index] = index
		selected_part.load_materials()
		load_materials(selected_part)



func new_color():
	$\"../color creator\".new_material()
	await $\"../color creator\".hidden
	selected_part.color_indexes[current_index] = Ship.color_scheme.size() - 1
	selected_part.load_materials()
	load_materials(selected_part)
"

