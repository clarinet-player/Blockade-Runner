script/source = "extends VBoxContainer



func _ready():
	update()



func update():
	for child in get_children():
		if child == $HBoxContainer:
			continue
		child.queue_free()
	
	for key in GameManager.ships.keys():
		var button = Button.new()
		button.theme = load(\"res://assets/theme2.tres\")
		button.text = key
		button.button_down.connect(load_file.bind([GameManager.read_file(GameManager.ships[key]), key]))
		$\".\".add_child(button)


func load_file(file : Array):
	hide()
	Ship.load_data(file[0], file[1])
	get_parent().load_ship()
	$\"../save file\".update()

"

[sub_resource type="GDScript" id="GDScript_3b3c2"]
script/source = "extends HBoxContainer


func _ready():
	update()

func update():
	$LineEdit.text = Ship.ship_name


func _on_save_button_down():
	if FileAccess.file_exists(\"user://\" + $LineEdit.text + \".json\"):
		$\"../overwrite\".show()
		hide()
	else:
		save()
		hide()


func save():
	Ship.ship_name = $LineEdit.text
	GameManager.write_file(\"user://\" + $LineEdit.text + \".json\", Ship.save_data())
	GameManager.ships.merge({$LineEdit.text: \"user://\" + $LineEdit.text + \".json\"})
	GameManager.write_file(\"user://ship_saves.json\", GameManager.ships)
	$\"../overwrite\".hide()
	$\"../saved\".show()
	await get_tree().create_timer(1.0).timeout
	$\"../saved\".hide()
	$\"../load file\".update()


func _on_cancel_button_down():
	hide()
"

